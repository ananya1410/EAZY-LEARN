<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="css/isha/insideteamview.css">
    <script src="https://kit.fontawesome.com/42d50a4c3d.js" crossorigin="anonymous"></script>
</head>
<body>
    <div class="container">
        <div class="item1">
            <div class="profile">
                <i class="fa-solid fa-circle-user fa-5x"></i><br><br><br>
                <span class="user"><%=admin%></span>
            </div>
            <a class="links" href="/admin"><i class="fa-solid fa-pen"></i>   teams</a><br><br><br>
            <a class="links" href="/createtest/info"><i class="fa-solid fa-pen"></i>   create test</a><br><br><br>
            <a class="links"  href="/admin/assignment/"><i class="fa-solid fa-pen"></i>   Create assignment</a><br><br><br>
            <a class="links" href="/admin/viewresult"><i class="fa-solid fa-pen"></i> View result</a>
        </div>
        <div class="item2">
            <div class="teamsicn">
                <div class="teamNo members">
                    <h3>Team Members</h3>
                    
                        <% users.forEach(user => { %>
                            <p><%= user %></p>
                        <% }) %>
                        <!-- <li>Isha</li>
                        <li>Aman</li>
                        <li>Sambhav</li>
                        <li>Aditi</li> -->
                    
                </div>
                <div class="teamNo announcements">
                    <h2>Activity</h2>
                    <% announcement.forEach(element =>{%>
                        <div class="subdiv"><p><%=element%></p></div>
                    <%});%>
                    
                    <div class="message">
                        <form action="/admin/teamid/<%=teamid%>" method="post">
                            <input type="text" name="message" autocomplete="off" class="input-message">
                            <input type="submit" class="input-msg-submit" value="POST">
                        </form>
                    </div>
                </div>
                
            </div>
        </div>
    </div>
</body>
</html>